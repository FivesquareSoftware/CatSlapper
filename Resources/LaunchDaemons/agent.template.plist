<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Disabled</key>
	<false/>
	<key>EnvironmentVariables</key>
	<dict>
		<key>CATALINA_BASE</key>
		<string>{CATALINA_BASE}</string>
		<key>CATALINA_HOME</key>
		<string>{CATALINA_HOME}</string>
		<key>CATALINA_OPTS</key>
		<string>{CATALINA_OPTS}</string>
		<key>JAVA_HOME</key>
		<string>{JAVA_HOME}</string>
	</dict>
	<key>Label</key>
	<string>org.apache.tomcat.{NAME}</string>
	<key>OnDemand</key>
	<true/>
	<key>Program</key>
	<string>{CATALINA_HOME}/bin/catalina.sh</string>
	<key>ProgramArguments</key>
	<array>
		<string>catalina.sh</string>
		<string>run</string>
	</array>
	<key>RunAtLoad</key>
	<true/>
	<key>ServiceDescription</key>
	<string>{NAME} Tomcat Server</string>
	<key>StandardErrorPath</key>
	<string>{CATALINA_HOME}/logs/catalina.out</string>
	<key>StandardOutPath</key>
	<string>{CATALINA_HOME}/logs/catalina.out</string>
</dict>
</plist>
